		var nome = source.u_nome.replace(" ",".").toLowerCase();
		var sobrenome = source.u_sobrenome.replace(" ",".").toLowerCase();
        target.email = nome + "." + sobrenome + "@example.com";
	}

})(source, target, map, log, action==="update");]]></script>
